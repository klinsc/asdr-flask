  "version": "0.2.0",
  "configurations": [
    {
      "name": "asdr.py",
      "type": "debugpy",
      "request": "launch",
      "program": "asdr.py",
      "console": "integratedTerminal",
      "justMyCode": true
    },
    {
      "name": "asdr.py-incorrect-tx",
      "type": "debugpy",
      "request": "launch",
      "program": "asdr.py",
      "console": "integratedTerminal",
      "justMyCode": true,
      "args": ["--debug=true", "--image_path=images/incorrect-tx.jpg"]
    },
    {
      "name": "asdr.py-missed-1-tx",
      "type": "debugpy",
      "request": "launch",
      "program": "asdr.py",
      "console": "integratedTerminal",
      "justMyCode": true,
      "args": ["--debug=true", "--image_path=images/missed-1-tx.jpg"]
    },
    {
      "name": "asdr.py-missed-1-22dpm",
      "type": "debugpy",
      "request": "launch",
      "program": "asdr.py",
      "console": "integratedTerminal",
      "justMyCode": true,
      "args": ["--debug=true", "--image_path=images/missed-1-22dpm.jpg"]
    },
    {
      "name": "asdr.py-missed-ss-ll",
      "type": "debugpy",
      "request": "launch",
      "program": "asdr.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "args": ["--debug=true", "--image_path=images/missed-ss-ll.jpg"]
    },
    {
      "name": "test.py",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/test.py",
      "console": "integratedTerminal",
      "justMyCode": true
    },
    {
      "name": "inference-ASDR",
      "type": "debugpy",
      "request": "launch",
      "program": "inference.py",
      "console": "integratedTerminal",
      "justMyCode": true,
      "args": "--threshold=0.5 --config=configs/${config:MMYOLO_CONFIG}.py --checkpoint checkpoints/${config:MMYOLO_MODEL}.pth --image=${config:MMYOLO_IMAGE}"
    },
    {
      "name": "display.py",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/temp/display.py",
      "console": "integratedTerminal",
      "justMyCode": true
    }
  ]
}
